<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.rodionov.igor.mobiletest">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".view.categories.CategoryListActivity"
            android:label="@string/categories_label">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".view.restaurants.RestaurantListActivity"
            android:label="@string/restaurants_label">
        </activity>
        <activity
            android:name=".view.vacations.VacationListActivity"
            android:label="@string/vacations_label">
        </activity>
    </application>

</manifest>